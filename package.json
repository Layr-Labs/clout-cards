{
  "name": "clout-cards",
  "version": "1.0.0",
  "description": "EigenX TEE TypeScript Application",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "ts-node src/index.ts",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\" --names \"backend,frontend\" --prefix-colors \"blue,green\""
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@types/pg": "^8.15.6",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "pg": "^8.16.3",
    "prisma": "^6.19.0",
    "viem": "^2.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "eigenx"
  ]
}
